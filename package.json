{
  "name": "peter-saville",
  "description": "Peter saville artwork",
  "version": "1.0.0",
  "scripts": {
    "build-module": "browserify index.js | uglifyjs -c -m > index.min.js",
    "build-example": "browserify example/index.js > example/bundle.js",
    "build": "npm run build-module && npm run build-example",
    "start": "budo example/index.js:bundle.js -P -l -d example"
  },
  "main": "index.js",
  "author": "antoninlanglade",
  "license": "MIT",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "ptjs": "^0.2.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^9.4.7",
    "uglify-js": "^2.7.5"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoninlanglade/peter-saville-artwork.git"
  },
  "keywords": [
    "Peter",
    "saville",
    "artwork"
  ],
  "bugs": {
    "url": "https://github.com/antoninlanglade/peter-saville-artwork/issues"
  },
  "homepage": "https://github.com/antoninlanglade/peter-saville-artwork#readme"
}
